<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<ScrollPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="biovis.sierra.client.GUI.peakTabController">
   <content>
      <AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="1080.0" prefWidth="1920.0">
      	<children>
      		<GridPane fx:id="chartPane" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      			<columnConstraints>
      				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      				<ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      			</columnConstraints>
      			<rowConstraints>
      				<RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      			</rowConstraints>
      			<children>
      				<StackPane fx:id="logPane" prefHeight="150.0" prefWidth="200.0">

				</StackPane>
      				<StackPane prefHeight="150.0" prefWidth="200.0" GridPane.columnIndex="1">
      					<children>
      						<BarChart fx:id="histogram" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" title="Overlaps" StackPane.alignment="CENTER">
      							<xAxis>
      								<CategoryAxis label="Replicate" side="BOTTOM" />
      							</xAxis>
      							<yAxis>
      								<NumberAxis label="Percentage" side="LEFT" />
      							</yAxis>
      						</BarChart>
      					</children>
      				</StackPane>
      			</children>
      		</GridPane>
      	</children>
      </AnchorPane>
   </content>
</ScrollPane>
